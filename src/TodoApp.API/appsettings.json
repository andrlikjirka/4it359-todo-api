{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "example.com",
  "TaskCollector": {
    "EnableTaskCollector": true,
    "SweepInterval": 1000,
    "MinPriorityThreshold": 3
  }
}
